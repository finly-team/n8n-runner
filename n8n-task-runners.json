{
  "task-runners": [
    {
      "runner-type": "javascript",
      "health-check-server-port": "5681",
      "env-overrides": {
        "NODE_FUNCTION_ALLOW_BUILTIN": "crypto",
        "NODE_FUNCTION_ALLOW_EXTERNAL": ""
      }
    },
    {
      "runner-type": "python",
      "health-check-server-port": "5682",
      "env-overrides": {
        "PYTHONPATH": "/opt/runners/task-runner-python",
        "N8N_RUNNERS_STDLIB_ALLOW": "json,datetime,math,re,uuid,hashlib,base64,decimal",
        "N8N_RUNNERS_EXTERNAL_ALLOW": "numpy,pandas,requests,dateutil"
      }
    }
  ]
}
